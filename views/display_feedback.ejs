<!doctype html>

<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FeedBacks</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lobster&family=Montserrat:wght@700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.14.1/dist/css/uikit.min.css" />
    <link rel="stylesheet" href="styles/Dashboard.css">
</head>

<body>
    <nav class="navbar-custom ">
        <div class=" navi">
            <a id="dash_return" style="margin-top: 10px;"><img src="./Title.svg" style="padding: 5px 0px 0px 10px ;"></a>
            <button class="btn-custom" type="submit" id="logout">Log Out</button>
        </div>
    </nav>

    <div class="container">
        <% if(message.length > 0) { %>
            <% } %>
                <div class="card">
                    <div class="card-header">

                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table>
                                <tr>
                                    <th>Section</th>
                                    <th>Feedback</th>
                                    <th>Semester</th>
                                    <th>Subject</th>
                                </tr>
                                <%
                            if(sampleData.length > 0)
                            {
                                sampleData.forEach(function(data){
                            %>
                                    <tr>
                                        <td>
                                            <%= data.section %>
                                        </td>
                                        <td>
                                            <%= data.feedback %>
                                        </td>
                                        <td>
                                            <%= data.semester %>
                                        </td>
                                        <td>
                                            <%= data.subject %>
                                        </td>
                                    </tr>
                                    <%
                                });
                            }
                            else
                            {
                            %>
                                        <tr>
                                            <td colspan="5">No Data Found</td>
                                        </tr>
                                        <%
                            }
                            %>
                            </table>
                        </div>
                    </div>
                </div>

    </div>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script>
        $(document).ready(function() {
            $("#logout").click(function(e) {
                window.location.href = "/";
            })
            $('#dash_return').click(function(e) {
                window.location.href = "/";
            })
        });
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>

</html>